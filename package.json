{
  "name": "gymcore",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:api": "turbo dev --filter=@gymcore/api",
    "dev:web": "turbo dev --filter=@gymcore/web",
    "build:api": "turbo build --filter=@gymcore/api",
    "build:web": "turbo build --filter=@gymcore/web"
  },
  "devDependencies": {
    "turbo": "^2.4.0"
  },
  "packageManager": "npm@10.0.0"
}
